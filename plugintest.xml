  </target>
  
  <target name="clean" depends="clean.module.plugintest" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, plugin.build.jar.plugintest" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
